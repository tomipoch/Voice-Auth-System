# Environment variables for VoiceAuth App
# Default environment (development)

# Application Configuration
NODE_ENV=development
VITE_APP_NAME=VoiceAuth
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# Authentication Configuration
VITE_JWT_TOKEN_KEY=voiceauth_token
VITE_JWT_REFRESH_KEY=voiceauth_refresh_token
VITE_SESSION_TIMEOUT=3600000

# Audio Configuration
VITE_AUDIO_SAMPLE_RATE=16000
VITE_AUDIO_BIT_DEPTH=16
VITE_AUDIO_MAX_DURATION=30
VITE_AUDIO_MIN_DURATION=3

# Features Flags
VITE_ENABLE_VOICE_ENROLLMENT=true
VITE_ENABLE_VOICE_VERIFICATION=true
VITE_ENABLE_ADMIN_PANEL=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_ANALYTICS=false

# UI Configuration
VITE_DEFAULT_LANGUAGE=es
VITE_THEME=light
VITE_ENABLE_DARK_MODE=true

# Development Configuration
VITE_MOCK_API=false
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_SOURCE_MAPS=true

# Security
VITE_ENABLE_HTTPS=false
VITE_CORS_ENABLED=true