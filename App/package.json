{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:local": "vite --mode development --config vite.config.js",
    "dev:watch": "vite --mode development --host",
    "dev:network": "vite --mode development --host 0.0.0.0",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:staging": "vite preview --mode staging",
    "preview:prod": "vite preview --mode production",
    "start": "npm run dev",
    "start:dev": "npm run dev",
    "start:staging": "vite --mode staging",
    "start:prod": "vite --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"No tests configured yet\"",
    "test:watch": "echo \"Test watch mode not configured yet\"",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:cache": "rm -rf node_modules/.vite",
    "clean:all": "rm -rf dist node_modules/.vite node_modules package-lock.json",
    "typecheck": "tsc --noEmit",
    "env:check": "node scripts/check-env.cjs",
    "env:copy": "cp .env.example .env.local",
    "install:fresh": "npm run clean:all && npm install",
    "deploy:dev": "npm run build:dev && echo 'Deploy to development server'",
    "deploy:staging": "npm run build:staging && echo 'Deploy to staging server'",
    "deploy:prod": "npm run build:prod && echo 'Deploy to production server'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.9",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.6",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "npm:rolldown-vite@7.2.2"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.2"
  }
}
